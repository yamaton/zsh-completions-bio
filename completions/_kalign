        '--format[: Output format. \[Fasta\]]' \
        '--reformat[: Reformat existing alignment. \[NA\]]' \
        '--gpo[: Gap open penalty. \[5.5\]]' \
        '--gpe[: Gap extension penalty. \[2.0\]]' \
        '--tgpe[: Terminal gap extension penalty. \[1.0\]]' \
        '--version[: Prints version. \[NA\]]' \
        "*: :_files"

}

