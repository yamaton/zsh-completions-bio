#compdef epost

# Auto-generated with h2o

function _epost {
    _arguments \
        '-help[Print usage information.]' \
        '-db[Indicate that the identifiers are from the specified database.]' \
        '-id[Specify identifiers directly.]' \
        '-format[uid or acc(ession).]' \
        '-input[Read from filename instead of standard input (which is then free to receive pipeline state from other edirect commands).]':file:_files \
        '-label[Alias for query step.]' \
        "*: :_files"

}

_epost "$@"

