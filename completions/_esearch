        '-db[Entrez database name.]' \
        '-query[Query string.]' \
        '-q[Query string.]' \
        '-sort[Sort results by the given field.]' \
        '-days[Return only results from the past N days.]' \
        '-datetype[Which date field (in abbreviated form) to consider.]' \
        '-mindate[Start of date range.]' \
        '-maxdate[End of date range.]' \
        '-field[Query words individually in field.]' \
        '-pairs[Query overlapping word pairs.]' \
        '-spell[Correct misspellings in query.]' \
        '-help[Print usage information.]' \
