function _vcfprimers {
    local line state

    function _commands {
        local -a commands
        commands=(
        )
        _describe 'command' commands
    }
 

    _arguments -C \
        {-f,--fasta-reference}'[FASTA reference file to use to obtain primer sequences]' \
        {-l,--primer-length}'[The length of the primer sequences on each side of the variant]' \
        '*: :_files'

    case $state in
    (cmd)
        _commands
        ;;
    (subcmd)
        case $line[1] in
        esac
        ;;
     esac

}

