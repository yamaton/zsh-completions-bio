function _sql2csv {
    local line state

    function _commands {
        local -a commands
        commands=(
        )
        _describe 'command' commands
    }
 

    _arguments -C \
        {-h,--help}'[show this help message and exit]' \
        {-v,--verbose}'[Print detailed tracebacks when errors occur.]' \
        {-l,--linenumbers}'[Insert a column of line numbers at the front of the output. Useful when piping to grep or as a simple primary key.]' \
        {-V,--version}'[Display version information and exit.]' \
        '--db[An sqlalchemy connection string to connect to a database.]' \
        '--query[The SQL query to execute. Overrides FILE and STDIN.]' \
        {-e,--encoding}'[Specify the encoding of the input query file.]' \
        {-H,--no-header-row}'[Do not output column names.]' \
        '*: :_files'

    case $state in
    (cmd)
        _commands
        ;;
    (subcmd)
        case $line[1] in
        esac
        ;;
     esac

}

