        {-l,--min-align}'[the minimal alignment size \[1\]]' \
        {-s,--same}'[write properly-paired reads to this file]' \
        {-h,--hist}'[write the fragment size histogram to FILE]':file:_files \
        {-c,--cov}'[write the physical coverage to FILE]':file:_files \
        {-v,--verbose}'[display verbose output]' \
        '--help[display this help and exit]' \
        '--version[output version information and exit]' \
        '--db[specify path of database repository in FILE]':file:_files \
        '--library[specify library NAME for sqlite]' \
        '--strain[specify strain NAME for sqlite]' \
        '--species[specify species NAME for sqlite]' \
        "*: :_files"

}

