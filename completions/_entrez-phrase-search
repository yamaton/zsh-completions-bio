function _entrez-phrase-search {
    _arguments \
        '-help[Print usage information.]' \
        '-count[Just print counts for individual word pairs.]' \
        '-counts[Just print counts for individual word pairs.]' \
        '-string[Just print the final query string.]' \
        {-db,-database}'[Search the specified Entrez database (default: pubmed).]' \
        '-field[Search the specified field (default: TIAB, titles + abstracts).]' \
        "*: :_files"

}

