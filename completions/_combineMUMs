# Auto-generated with h2o

function _combineMUMs {
    _arguments \
        '-D[Only output to stdout the difference positions and characters]' \
        '-n[Allow matches only between nucleotides, i.e., ACGTs]' \
        '-N[Break matches at <num> or more consecutive non-ACGTs]' \
        '-q[Used to label query match]' \
        '-r[Used to label reference match]' \
        '-S[Output all differences in strings]' \
        '-t[Label query matches with query fasta header]' \
        '-v[Set verbose level for extra output]' \
        '-W[Reset the default output filename witherrors.gaps]':file:_files \
        '-x[Don'\''t output .cover files]' \
        '-e[Set error-rate cutoff to e (e.g. 0.02 is two percent)]' \
        "*: :_files"

}

_combineMUMs "$@"

