#compdef _vcfld vcfld

# Auto-generated with h2o


function _vcfld {
    local line state

    function _commands {
        local -a commands
        commands=(
        )
        _describe 'command' commands
    }
 

    _arguments -C \
        {-t,--target}'[a zero base comma separated list of target individuals corresponding to VCF columns]' \
        {-b,--background}'[a zero base comma separated list of background individuals corresponding to VCF columns]' \
        {-f,--file}'[a properly formatted phased VCF file]':file:_files \
        {-y,--type}'[type of genotype likelihood: PL, GL or GP]' \
        {-w,--window}'[window size to average LD; default is 1000]' \
        {-e,--external}'[population to calculate LD expectation; default is target]' \
        {-d,--derived}'[which haplotype to count "00" vs "11"; default "00",]' \
        '*: :_files'

    case $state in
    (cmd)
        _commands
        ;;
    (subcmd)
        case $line[1] in
        esac
        ;;
     esac

}

