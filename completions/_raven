function _raven {
    local line state

    function _commands {
        local -a commands
        commands=(
        )
        _describe 'command' commands
    }
 

    _arguments -C \
        {-k,--kmer-len}'[length of minimizers used to find overlaps \[default: 15\]]' \
        {-w,--window-len}'[length of sliding window from which minimizers are sampled \[default: 5\]]' \
        {-f,--frequency}'[threshold for ignoring most frequent minimizers \[default: 0.001\]]' \
        {-p,--polishing-rounds}'[number of times racon is invoked \[default: 2\]]' \
        {-m,--match}'[score for matching bases \[default: 3\]]' \
        {-n,--mismatch}'[score for mismatching bases \[default: -5\]]' \
        {-g,--gap}'[gap penalty (must be negative) \[default: -4\]]' \
        '--graphical-fragment-assembly[prints the assembly graph in GFA format]' \
        '--resume[resume previous run from last checkpoint]' \
        '--disable-checkpoints[disable checkpoint file creation]' \
        {-t,--threads}'[number of threads \[default: 1\]]' \
        '--version[prints the version number]' \
        {-h,--help}'[prints the usage]' \
        '*: :_files'

    case $state in
    (cmd)
        _commands
        ;;
    (subcmd)
        case $line[1] in
        esac
        ;;
     esac

}

