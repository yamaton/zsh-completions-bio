#compdef edirutil

# Auto-generated with h2o

function _edirutil {
    _arguments \
        '-db[Work with the named Entrez database.]' \
        '-web[Pass the specified WebEnv cookie.]' \
        '-key[Work with the Nth result set associated with the specified WebEnv.]' \
        '-count[Expect N results.]' \
        '-step[Indicate the current pipeline depth.]' \
        '-silent[Suppress connection failure retry messages.]' \
        '-verbose[Display the ENTREZ_DIRECT XML field values at each step.]' \
        '-debug[Print the internal URL query and XML results of each step.]' \
        "*: :_files"

}

_edirutil "$@"

