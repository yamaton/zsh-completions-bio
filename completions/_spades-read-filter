# Auto-generated with h2o

function _spades-read-filter {
    _arguments \
        {-k,--kmer}'[K-mer length]' \
        {-c,--cov}'[Median kmer count threshold (read pairs, s.t. kmer count median for BOTH reads LESS OR EQUAL to this value will be ignored)]' \
        {-d,--dataset}'[Dataset description (in YAML)]' \
        {-t,--threads}'[# of threads to use]' \
        {-o,--outdir}'[Output directory to use]':file:_files \
        '--drop-names[Drop read names and quality (makes everything faster)]' \
        '--drop-quality[Drop read quality (makes everything faster)]' \
        {-h,--help}'[Show help]' \
        "*: :_files"

}

_spades-read-filter "$@"

