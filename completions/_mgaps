function _mgaps {
    _arguments \
        '-C[Check that fasta header labels alternately have "Reverse"]' \
        '-d[Fixed diagonal difference to join matches]' \
        '-e[Use extent of match (end - start) rather than sum of piece lengths to determine length of cluster]' \
        '-f[Fraction of separation for diagonal difference]' \
        '-l[Minimum length of cluster match]' \
        '-s[Maximum separation between matches in cluster]' \
        "*: :_files"

}

