#compdef spades-gbuilder

# Auto-generated with h2o

function _spades-gbuilder {
    _arguments \
        '-k[k-mer length to use]' \
        '-c[infer coverage]' \
        '-t[# of threads to use]' \
        '-tmp-dir[scratch directory to use]':file:_files \
        '-b[sorting buffer size, per thread]' \
        '--unitigs[produce unitigs (default)]' \
        '--fastg[produce graph in FASTG format]' \
        '--gfa[produce graph in GFA1 format]' \
        '--spades[produce graph in SPAdes internal format]' \
        "*: :_files"

}

_spades-gbuilder "$@"

