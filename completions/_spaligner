function _spaligner {
    local line state

    function _commands {
        local -a commands
        commands=(
        )
        _describe 'command' commands
    }
 

    _arguments -C \
        {-d,--datatype}'[type of sequences: nanopore, pacbio]' \
        {-s,--sequences}'[path to fasta/fastq file with sequences]' \
        {-g,--graph}'[path to GFA-file or SPAdes saves folder]' \
        {-k,--kmer}'[graph k-mer size (odd value)]' \
        {-t,--threads}'[# of threads to use]' \
        {-o,--outdir}'[output directory]':file:_files \
        '*: :_files'

    case $state in
    (cmd)
        _commands
        ;;
    (subcmd)
        case $line[1] in
        esac
        ;;
     esac

}

