# Auto-generated with h2o

function _bFst {
    _arguments \
        {-t,--target}'[a zero bases comma separated list of target individuals corrisponding to VCF columns]' \
        {-b,--background}'[a zero bases comma separated list of background individuals corrisponding to VCF columns]' \
        {-f,--file}'[a proper formatted VCF file. the FORMAT field MUST contain "PL"]':file:_files \
        {-d,--deltaaf}'[skip sites were the difference in allele frequency is less than deltaaf]' \
        "*: :_files"

}

_bFst "$@"

