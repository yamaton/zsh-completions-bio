function _brename {
    local line state

    function _commands {
        local -a commands
        commands=(
        )
        _describe 'command' commands
    }
 

    _arguments -C \
        {-d,--dry-run}'[print rename operations but do not run]' \
        {-F,--exclude-filters}'[exclude file filter(s) (regular expression, NOT wildcard). multiple values supported, e.g., -F ".html" -F ".htm", but ATTENTION: comma in filter is treated as separator of multiple filters]' \
        {-U,--force-undo}'[continue undo even when some operations failed]' \
        {-h,--help}'[help for brename]' \
        {-i,--ignore-case}'[ignore case of -p/--pattern, -f/--include-filters and -F/--exclude-filters]' \
        {-e,--ignore-ext}'[ignore file extension. i.e., replacement does not change file extension]' \
        {-f,--include-filters}'[include file filter(s) (regular expression, NOT wildcard). multiple values supported, e.g., -f ".html" -f ".htm", but ATTENTION: comma in filter is treated as separator of multiple filters (default \[.\])]' \
        {-D,--including-dir}'[rename directories]' \
        {-K,--keep-key}'[keep the key as value when no value found for the key]' \
        {-I,--key-capt-idx}'[capture variable index of key (1-based) (default 1)]' \
        {-m,--key-miss-repl}'[replacement for key with no corresponding value]' \
        {-k,--kv-file}'[tab-delimited key-value file for replacing key with value when using "{kv}" in -r (--replacement)]' \
        {-l,--list}'[only list paths that match pattern]' \
        {-a,--list-abs}'[list absolute path, using along with -l/--list]' \
        {-s,--list-sep}'[separator for list of found paths (default "\n")]' \
        '--max-depth[maximum depth for recursive search (0 for no limit)]' \
        {-N,--nature-sort}'[list paths in nature sort, using along with -l/--list]' \
        '--nr-width[minimum width for {nr} in flag -r/--replacement. e.g., formating "1" to "001" by --nr-width 3 (default 1)]' \
        '--only-dir[only rename directories]' \
        {-o,--overwrite-mode}'[overwrite mode (0 for reporting error, 1 for overwrite, 2 for not renaming) (default 0)]' \
        {-p,--pattern}'[search pattern (regular expression)]' \
        {-q,--quiet}'[be quiet, do not show information and warning]' \
        {-R,--recursive}'[rename recursively]' \
        {-r,--replacement}'[replacement. capture variables supported. e.g. $1 represents the first submatch. ATTENTION: for *nix OS, use SINGLE quote NOT double quotes or use the \ escape character. Ascending integer is also supported by "{nr}"]' \
        {-n,--start-num}'[starting number when using {nr} in replacement (default 1)]' \
        {-u,--undo}'[undo the LAST successful operation]' \
        {-v,--verbose}'[verbose level (0 for all, 1 for warning and error, 2 for only error) (default 0)]' \
        {-V,--version}'[print version information and check for update]' \
        '*: :_files'

    case $state in
    (cmd)
        _commands
        ;;
    (subcmd)
        case $line[1] in
        esac
        ;;
     esac

}

