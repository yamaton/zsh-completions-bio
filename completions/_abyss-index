#compdef abyss-index

# Auto-generated with h2o

function _abyss-index {
    _arguments \
        '--both[build both FAI and FM indexes \[default\]]' \
        '--fai[build a FAI index]' \
        '--fm[build a FM index]' \
        '--fa2bwt[build the BWT directly without the SA]' \
        '--bwt2fm[build the FM index from the BWT]' \
        {-a,--alphabet}'[use the alphabet STRING \[-ACGT\]]' \
        '--alpha[equivalent to -a'\'' ABCDEFGHIJKLMNOPQRSTUVWXYZ'\'']' \
        '--dna[equivalent to -a'\''-ACGT'\'']' \
        '--protein[equivalent to -a'\''#*ACDEFGHIKLMNPQRSTVWY'\'']' \
        {-s,--sample}'[sample the suffix array \[16\]]' \
        {-d,--decompress}'[decompress the index FILE]' \
        {-c,--stdout}'[write output to standard output]' \
        {-v,--verbose}'[display verbose output]' \
        '--help[display this help and exit]' \
        '--version[output version information and exit]' \
        "*: :_files"

}

_abyss-index "$@"

