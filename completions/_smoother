#compdef _smoother smoother

# Auto-generated with h2o


function _smoother {
    local line state

    function _commands {
        local -a commands
        commands=(
        )
        _describe 'command' commands
    }
 

    _arguments -C \
        {-f,--file}'[a file created by GPAT++]':file:_files \
        {-o,--format}'[format of input file, case sensitive. available format options: {wcFst, pFst, bFst, iHS, xpEHH, abba-baba, col3}]' \
        {-w,--window}'[size of genomic window in base pairs (default 5000)]' \
        {-s,--step}'[window step size in base pairs (default 1000)]' \
        {-t,--truncate}'[end last window at last position (zero based)]' \
        '*: :_files'

    case $state in
    (cmd)
        _commands
        ;;
    (subcmd)
        case $line[1] in
        esac
        ;;
     esac

}

