# Auto-generated with h2o

function _fetch-pubmed {
    _arguments \
        {-strict,-mixed}'[Run rchive(1) with -flag strict or -flag mixed, respectively.]' \
        '-path[Place the local archive in dir.]':file:_files \
        '-all[Dump the entire archive.]' \
        '-fresh[Dump the entire archive into filenames of the form pubmed001.xml.gz, using any existing such files as a starting point.]' \
        "*: :_files"

}

_fetch-pubmed "$@"

