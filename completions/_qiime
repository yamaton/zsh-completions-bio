function _qiime_tools_replay-citations {
    _arguments \
        '--in-fp[filepath to a QIIME 2 Archive (.qza or .qzv) or directory of Archives \[required\]]' \
        {--recurse,--no-recurse}'[if in-fp is a directory, will also search sub-directories when finding Archives to parse \[default: no-recurse\]]' \
        {--deduplicate,--no-deduplicate}'[If deduplicate, duplicate citations will be removed heuristically, e.g. by comparing DOI fields. This greatly reduces manual curation of reference lists, but introduces a small risk of reference loss. \[default: deduplicate\]]' \
        {--suppress-header,--no-suppress-header}'[do not write header/footer blocks in the output script]' \
        {--verbose,--no-verbose}'[print status messages to stdout while processing]' \
        '--out-fp[the filepath where your bibtex file should be written]':file:_files \
        '--help[Show this message and exit.]' \
        "*: :_files"

}

function _qiime_tools_replay-provenance {
    _arguments \
        '--in-fp[filepath to a QIIME 2 Archive (.qza or .qzv) or directory of Archives \[required\]]' \
        {--recurse,--no-recurse}'[if in-fp is a directory, will also search sub-directories when finding Archives to parse \[default: no-recurse\]]' \
        '--usage-driver[the target interface for your replay script \[default: cli\]]' \
        {--validate-checksums,--no-validate-checksums}'[check that replayed archives are intact and uncorrupted]' \
        {--parse-metadata,--no-parse-metadata}'[parse the original metadata captured in provenance for review or replay \[default: parse-metadata\]]' \
        {--use-recorded-metadata,--no-use-recorded-metadata}'[re-use the original metadata captured in provenance]' \
        {--suppress-header,--no-suppress-header}'[do not write header/footer blocks in the output script]' \
        {--verbose,--no-verbose}'[print status messages to stdout while processing]' \
        {--dump-recorded-metadata,--no-dump-recorded-metadata}'[write the original metadata captured in provenance to disk in the --metadata-out-dir directory]' \
        '--metadata-out-dir[the directory where captured study metadata should be written if --dump-recorded-metadata. This often produces many outputs, so a dedicated directory should generally be used. Creates the directory if it does not already exist. By default, metadata is written to `${PWD}/recorded_metadata/`]' \
        '--out-fp[the filepath where your replay script should be written]':file:_files \
        '--help[Show this message and exit.]' \
        "*: :_files"

}

function _qiime_tools_replay-supplement {
    _arguments \
        '--in-fp[filepath to a QIIME 2 Archive or directory of Archives]' \
        {--recurse,--no-recurse}'[if in-fp is a directory, will also search sub-directories when finding .qza/.qzv files to parse \[default: no-recurse\]]' \
        {--deduplicate,--no-deduplicate}'[If deduplicate, duplicate citations will be removed heuristically, e.g. by comparing DOI fields. This greatly reduces manual curation of reference lists, but introduces a small risk of reference loss. \[default: deduplicate\]]' \
        {--validate-checksums,--no-validate-checksums}'[check that replayed archives are intact and uncorrupted]' \
        {--parse-metadata,--no-parse-metadata}'[parse the original metadata captured in provenance for review or replay \[default: parse-metadata\]]' \
        {--use-recorded-metadata,--no-use-recorded-metadata}'[re-use the original metadata captured in provenance]' \
        {--suppress-header,--no-suppress-header}'[do not write header/footer blocks in the output script]' \
        {--verbose,--no-verbose}'[print status messages to stdout while processing]' \
        {--dump-recorded-metadata,--no-dump-recorded-metadata}'[write the original metadata captured in provenance to recorded_metadata/ inside the archive]' \
        '--out-fp[the filepath where your reproduciblity supplement zipfile should be written \[required\]]':file:_files \
        '--help[Show this message and exit.]' \
        "*: :_files"

}

