        {-h,--help}'[show this help message and exit]' \
        {-r,--ref_name}'[Reference to process, only needed if bams contain multiple references. (default: None)]' \
        {-o,--output_prefix}'[Prefix for outputs. (default: trimmed)]' \
        {-f,--reference_fasta}'[Reference fasta to trim to alignment window. (default: None)]' \
        '*: :_files'

    case $state in
    (cmd)
        _commands
        ;;
