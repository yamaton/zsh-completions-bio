        '-help[Print usage information, complete with a few examples, some additionally illustrating the use of xtract(1).]' \
        '-ftp[Use FTP instead of HTTP. The shell implementation accepts a filename on the command line only when invoked from a (pseudo)terminal, and otherwise reads filenames from standard input.]':file:_files \
        '-get[Use HTTP(S) GET instead of POST.]' \
        '-url[Base URL for external search.]' \
        '-lst[Lists contents of FTP site.]':file:_files \
        '-dwn[Like -ftp, but saves contents to correspondingly named files in the current directory.]':file:_files \
        '-asp[Like -dwn, but uses Aspera'\''s proprietary client if installed.]':file:_files \
        '-tag[Add &tag=value,... to the base URL. (Must follow any simple components.)]' \
        '-examples[Examples of advanced queries.]' \
        '-version[Print version number.]' \
        '*: :_files'

    case $state in
    (cmd)
        _commands
        ;;
    (subcmd)
        case $line[1] in
        esac
        ;;
     esac

}

