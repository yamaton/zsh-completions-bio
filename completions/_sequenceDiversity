function _sequenceDiversity {
    _arguments \
        {-t,--target}'[a zero base comma separated list of target individuals corresponding to VCF columns]' \
        {-f,--file}'[a properly formatted phased VCF file]':file:_files \
        {-y,--type}'[type of genotype likelihood: PL, GL or GP]' \
        {-a,--af}'[sites less than af are filtered out; default is 0]' \
        {-r,--region}'[a tabix compliant region : "seqid:0-100" or "seqid"]' \
        {-w,--window}'[the number of SNPs per window; default is 20]' \
        "*: :_files"

}

