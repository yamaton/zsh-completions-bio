# Auto-generated with h2o

function _nextPolish {
    _arguments \
        {-l,--log}'[log file (default: pidXXX.log.info)]':file:_files \
        {-v,--version}'[show program'\''s version number and exit]' \
        {-h,--help}'[please use the config file to pass parameters]' \
        "*: :_files"

}

_nextPolish "$@"

