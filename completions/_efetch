#compdef efetch

# Auto-generated with h2o

function _efetch {
    _arguments \
        '-format[Format of record or report. (See -help output for examples.)]' \
        '-mode[text, xml, asn.1, or json.]' \
        '-style[withparts or conwithfeat.]' \
        '-db[Entrez database name for immediate lookups.]' \
        '-id[Unique identifier or accession for immediate lookups.]' \
        '-seq_start[First sequence position to retrieve (1-based).]' \
        '-seq_stop[Last sequence position to retrieve (1-based).]' \
        '-strand[Strand of DNA to retrieve: 1 for plus, 2 for minus.]' \
        '-chr_start[First sequence position to retrieve (0-based).]' \
        '-chr_stop[Last sequence position to retrieve (1-based).]' \
        '-complexity[How much context to fetch: 0 default 1 Bioseq 3 Nuc-prot set]' \
        '-extend[Extend sequence retrieval by N residues in both directions.]' \
        '-extrafeat[Bit flag specifying extra features.]' \
        '-raw[Skip database-specific XML modifications.]' \
        '-json[Convert adjusted XML output to JSON.]' \
        '-help[Print usage information, complete with examples of notable -db/-format/-mode combinations.]' \
        "*: :_files"

}

_efetch "$@"

