function _unikmer_uniqs {
    _arguments \
        {-M,--allow-multiple-mapped-kmer}'[allow multiple mapped k-mers]' \
        '--circular[circular genome. type "unikmer uniqs -h" for details]' \
        {-g,--genome}'[genomes in (gzipped) fasta file(s)]' \
        {-h,--help}'[help for uniqs]' \
        {-x,--max-cont-non-uniq-kmers}'[max continuous non-unique k-mers]' \
        {-X,--max-num-cont-non-uniq-kmers}'[max number of continuous non-unique k-mers]' \
        {-m,--min-len}'[minimum length of subsequence (default 200)]' \
        {-o,--out-prefix}'[out file prefix ("-" for stdout) (default "-")]' \
        {-a,--output-fasta}'[output fasta format instead of BED3]' \
        {-B,--seq-name-filter}'[list of regular expressions for filtering out sequences by header/name, case ignored]' \
        {-W,--seqs-in-a-file-as-one-genome}'[treat seqs in a genome file as one genome]' \
        {-c,--compact}'[write compact binary file with little loss of speed]' \
        '--compression-level[compression level (default -1)]' \
        '--data-dir[directory containing NCBI Taxonomy files, including nodes.dmp, names.dmp, merged.dmp and delnodes.dmp (default "~/.unikmer")]' \
        {-I,--ignore-taxid}'[ignore taxonomy information]' \
        {-i,--infile-list}'[file of input files list (one file per line), if given, they are appended to files from cli arguments]' \
        '--max-taxid[for smaller TaxIds, we can use less space to store TaxIds. default value is 1<<32-1, that'\''s enough for NCBI Taxonomy TaxIds (default 4294967295)]' \
        {-C,--no-compress}'[do not compress binary file (not recommended)]' \
        '--nocheck-file[do not check binary file, when using process substitution or named pipe]' \
        {-j,--threads}'[number of CPUs to use (default 4)]' \
        '--verbose[print verbose information]' \
        "*: :_files"

}

